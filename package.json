{
  "name": "khaos-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "start:qa": "env-cmd -f .env.qa npm run-script start",
    "start:testnet": "env-cmd -f .env.testnet npm run-script start",
    "build": "react-scripts build",
    "build:qa": "env-cmd -f .env.qa npm run-script build",
    "build:testnet": "env-cmd -f .env.testnet npm run-script build",
    "build:production": "env-cmd -f .env.qa npm run-script build",
    "docker:qa:build": "docker build -t gladiatorsfinance/frontend:qa -f DockerfileQA .",
    "docker:testnet:build": "docker build -t gladiatorsfinance/frontend:testnet -f DockerfileTestnet .",
    "docker:production:build": "docker build -t gladiatorsfinance/frontend:production -f Dockerfile .",
    "docker:qa:push": "docker push gladiatorsfinance/frontend:qa",
    "docker:testnet:push": "docker push gladiatorsfinance/frontend:testnet",
    "docker:production:push": "docker push gladiatorsfinance/frontend:production",
    "test": "react-scripts test --testPathIgnorePatterns=/src/__tests__/config/",
    "test:config": "react-scripts test --testPathPattern=/src/__tests__/config/",
    "integration-test": "start-server-and-test 'serve -s build -p 3000' http://localhost:3000 'cypress run'",
    "eject": "react-scripts eject",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "format:check": "prettier --check '**/*.{js,jsx,ts,tsx}'",
    "format:write": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "updateLPsAPR": "NODE_PATH=./src ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/updateLPsAPR.ts",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@binance-chain/bsc-connector": "^1.0.0",
    "@ethersproject/experimental": "^5.0.1",
    "@pancakeswap/uikit": "file:../toolkit/packages/pancake-uikit",
    "@pinata/sdk": "^1.1.23",
    "@react-three/drei": "^7.5.0",
    "@react-three/fiber": "^7.0.6",
    "@reduxjs/toolkit": "^1.5.0",
    "@sentry/react": "^6.17.2",
